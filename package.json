{
  "name": "cycle-mongoose",
  "version": "1.0.0",
  "description": "Mongoose.js driver for Cycle.js.",
  "keywords": ["mongodb", "mongoose", "cyclejs", "driver"],
  "main": "lib/index.js",
  "scripts": {
