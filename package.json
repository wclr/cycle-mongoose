    "cycle-async-driver": "^1.0.0"
  },
  "devDependencies": {
    "@cycle/core": "^6.0.0",
    "@cycle/isolate": "^1.2.0",
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "rx": "^4.0.7",
    "rx-factory": "^1.0.0",
    "tape": "^4.4.0"
  }
